{
  "name": "bevy-diagnostic-explorer",
  "publisher": "xyzw-io",
  "author": {
    "name": "Vladimir Zaytsev",
    "email": "vladimir@xyzw.io"
  },
  "icon": "assets/bevy.png",
  "displayName": "Bevy Diagnostics Explorer",
  "description": "Diagnostics Explorer for Bevy Engine",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Testing",
    "Visualization"
  ],
  "keywords": [
    "rust",
    "bevy",
    "diagnostics",
    "ecs",
    "systems",
    "profiler",
    "debugger",
    "explorer",
    "game-dev",
    "game"
  ],
  "homepage": "https://xyzw.io",
  "bugs": {
    "url": "https://xyzw.io"
  },
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished"
  ],
  "os": [
    "darwin",
    "linux",
    "windows"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "bevyProfilerContainer": [
        {
          "id": "viewDiagnostics",
          "name": "Bevy Diagnostics"
        },
        {
          "id": "viewHelp",
          "name": "Help & Feedback"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bevyProfilerContainer",
          "title": "Bevy",
          "icon": "assets/bevy2.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.1"
  }
}
